<launch>
    <include file="$(find dnetwork)/launch/0.launch"/>
    <node pkg="dviz"
          name="dviz_$(env ZJUDANCER_ROBOTID)"
          type="dviz"
          output="screen"
          respawn="false"
          required="true"
          cwd="node"
          args="">
        <env name="ROSCONSOLE_CONFIG_FILE" value="$(find dlaunch)/drosconsole.conf"/>
        <param name="RobotId" value="0" />
    </node>
</launch>
